package com.dam.authentication;

public enum Role {
	
	INVESTOR ("Investor"),
	USER ("Any other user"),
	ADMIN ("Administrator"),
	ROOT_ADMIN ("Root Administrator"),
	CONTROLLER ("Controller");
	
	private String description;

	Role () {
	}
	
	Role (String description) {
		this.description = description;
	}
	
	public String getDescription() {
		return description;
	}

}
